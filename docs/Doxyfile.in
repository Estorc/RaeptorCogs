# docs/Doxyfile.in
PROJECT_NAME           = "@PROJECT_NAME@"
PROJECT_BRIEF          = "API reference for @PROJECT_NAME@"
OUTPUT_DIRECTORY       = "@CMAKE_BINARY_DIR@/docs/doxygen"
GENERATE_HTML          = NO
GENERATE_XML           = YES
XML_OUTPUT             = xml
RECURSIVE              = YES
INPUT                  = @CMAKE_SOURCE_DIR@/include @CMAKE_SOURCE_DIR@/src
EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
EXTRACT_STATIC         = YES
EXTRACT_LOCAL_CLASSES  = YES
WARN_IF_UNDOCUMENTED   = NO
OPTIMIZE_OUTPUT_JAVA   = NO
GENERATE_LATEX         = NO
ENABLE_PREPROCESSING   = YES
MACRO_EXPANSION        = YES
EXPAND_ONLY_PREDEF     = NO
QUIET                  = YES
OPTIMIZE_OUTPUT_FOR_C  = NO
OPTIMIZE_OUTPUT_JAVA   = NO
OPTIMIZE_OUTPUT_VHDL   = NO
INLINE_SOURCES         = YES
HIDE_FRIEND_COMPOUNDS  = YES
EXCLUDE_SYMBOLS        = friend*
EXTRACT_ANON_NSPACES   = NO
EXTRACT_LOCAL_CLASSES  = NO
EXTRACT_LOCAL_METHODS  = NO
PREDEFINED             = VKAPI_ATTR= VKAPI_CALL=
ALIASES                = "module=\par Module^^"